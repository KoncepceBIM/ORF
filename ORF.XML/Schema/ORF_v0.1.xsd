      <xs:enumeration value="RADIAN">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
            Veličina: rovinný úhel
            Symbol: rad
            Výraz: rad = m/m = 1
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="STERADIAN">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
            Veličina: prostorový úhel
            Symbol: sr
            Výraz: sr = m2/m2 = 1
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="HERTZ">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
            Veličina: frekvence
            Symbol: Hz
            Výraz: Hz = s−1
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="NEWTON">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
            Veličina: síla
            Symbol: N
            Výraz: N = kg · m/s2
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="PASCAL">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
            Veličina: tlak
