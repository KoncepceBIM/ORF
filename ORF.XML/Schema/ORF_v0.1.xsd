      <xs:enumeration value="JOULE">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
            Veličina: energie
            Symbol: J
            Výraz: J = N · m
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="WATT">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
