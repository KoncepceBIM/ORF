        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="FARAD">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
            Veličina: elektrická kapacita
            Symbol: F
            Výraz: F = C/V
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="OHM">
