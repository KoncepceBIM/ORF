  <xs:complexType name="TJednotka" abstract="true">
    <xs:attribute name="Id" type="xs:ID"></xs:attribute>
  </xs:complexType>
  <xs:complexType name="TPojmenovanaJednotka" abstract="true">
    <xs:complexContent>
      <xs:extension base="TJednotka">

      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TJednotkaSI">
    <xs:complexContent>
      <xs:extension base="TPojmenovanaJednotka">
        <xs:attribute name="Nazev" type="TNazevJednotkySI" use="required"/>
        <xs:attribute name="Predpona" type="TPredponaSI" use="optional"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TOdvozenaJednotka">
    <xs:complexContent>
      <xs:extension base="TJednotka">
        <xs:sequence>
          <xs:element name="Componenta" minOccurs="1" maxOccurs="unbounded">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Jednotka" type="TPojmenovanaJednotka" minOccurs="1"/>
              </xs:sequence>
              <xs:attribute name="Exponent" type="xs:integer" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TKonverzniJednotka">
    <xs:complexContent>
      <xs:extension base="TPojmenovanaJednotka">
        <xs:sequence>
          <xs:element name="ZakladniJednotka" type="TJednotkaSI" minOccurs="1"/>
        </xs:sequence>
        <xs:attribute name="Nazev" type="xs:string" use="required"/>
        <xs:attribute name="Symbol" type="xs:string" use="required"/>
        <xs:attribute name="Meritko" type="xs:double" default="1"/>
        <xs:attribute name="Odsazeni" type="xs:double" default="0"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TJednotkaZavislaNaKontextu">
    <xs:complexContent>
      <xs:extension base="TJednotka">
        <xs:attribute name="Nazev" type="xs:string" use="required"/>
        <xs:attribute name="Symbol" type="xs:string" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TPenezniJednotka">
    <xs:complexContent>
      <xs:extension base="TJednotka">
        <xs:attribute name="Mena" type="TMena"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:complexType name="TORF">
    <xs:sequence>
      <xs:element name="Projekt" type="TProjekt"/>
      <xs:element name="Rozpocet" type="TRozpocet"/>
      <xs:element name="Definice">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Klasifikace" type="TKlasifikace" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="CenovaSoustava" type="TCenovaSoustava" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="Jednotka" type="TJednotka" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
          <xs:attribute name="ZaokrouhleniMnozstviPolozky" type="TZaokrouhleni" use="required"/>
          <xs:attribute name="PozadovanaSazbaDPH" type="xs:boolean" use="required"/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="Verze" type="TVerze" />
  </xs:complexType>
  <xs:complexType name="TCenovaSoustava">
    <xs:attribute name="Id" type="xs:string" use="required"/>
    <xs:attribute name="Nazev" type="xs:string" use="required"/>
    <xs:attribute name="Popis" type="xs:string" use="optional"/>
    <xs:attribute name="Zdroj" type="xs:string" use="optional"/>
    <xs:attribute name="Umisteni" type="TUrl" use="optional"/>
    <xs:attribute name="Edice" type="xs:string" use="optional"/>
  </xs:complexType>
  <xs:complexType name="TCenovaSoustavaRef">
    <xs:attribute name="Ref" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="TKlasifikace">
    <xs:sequence>
      <xs:element name="Trida" type="TTrida" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="Typ" type="TTypKlasifikace" default="Vlastni"/>
    <xs:attribute name="Nazev" type="xs:string" use="required"/>
    <xs:attribute name="Popis" type="xs:string" use="optional"/>
    <xs:attribute name="Zdroj" type="xs:string" use="optional"/>
    <xs:attribute name="Umisteni" type="TUrl" use="optional"/>
    <xs:attribute name="Edice" type="xs:string" use="optional"/>
  </xs:complexType>
  <xs:complexType name="TTrida">
    <xs:sequence>
      <xs:element name="Trida" type="TTrida" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="Id" type="xs:string" use="required"/>
    <xs:attribute name="Kod" type="xs:string" use="required"/>
    <xs:attribute name="Nazev" type="xs:string" use="optional"/>
    <xs:attribute name="Popis" type="xs:string" use="optional"/>
  </xs:complexType>
  <xs:complexType name="TTridaRef">
    <xs:attribute name="Ref" type="xs:string"/>
  </xs:complexType>
  <xs:complexType name="TIdentita" abstract="true">
    <xs:sequence>
      <xs:element name="Popis" type="xs:string" minOccurs="0"/>
      <xs:element name="ExterniEntita" type="TExterniEntita" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="Id" type="TGuid" use="required"/>
    <xs:attribute name="Nazev" type="xs:string" use="required"/>
    <xs:attribute name="DlouhyNazev" type="xs:string" use="optional"/>
  </xs:complexType>
  <xs:complexType name="TExterniEntita">
    <xs:attribute name="Id" type="xs:string" use="required"/>
    <xs:attribute name="Nazev" type="xs:string" use="optional"/>
    <xs:attribute name="Zdroj" type="xs:string" use="optional"/>
    <xs:attribute name="Umisteni" type="TUrl" use="optional"/>
  </xs:complexType>
  <xs:complexType name="TProjekt">
    <xs:complexContent>
      <xs:extension base="TIdentita">
        <xs:sequence>
          <xs:element name="Adresa" minOccurs="0" type="xs:string"/>
          <xs:element name="Umisteni" minOccurs="0" type="TPoloha"/>
        </xs:sequence>

        
        <xs:attribute name="EvidencniCislo" type="xs:string" use="optional"/>


        <xs:attribute name="DatumZahajeniRealizace" type="xs:date" use="optional"/>
        <xs:attribute name="DatumUkonceniRealizace" type="xs:date" use="optional"/>

        <xs:attribute name="Mena" type="TMena" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TPoloha">
    <xs:attribute name="ZemSirka" type="xs:double" use="required"/>
    <xs:attribute name="ZemDelka" type="xs:double" use="required"/>
  </xs:complexType>
  <xs:complexType name="TOsoba">
    <xs:sequence>
      <xs:element name="Adresa" type="xs:string" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="Jmeno" type="xs:string" use="required"/>
    <xs:attribute name="Email" type="TEmail" use="optional"/>
  </xs:complexType>
  <xs:complexType name="TOrganizace">
    <xs:sequence>
      <xs:element name="Adresa" type="xs:string" minOccurs="0"/>
      <xs:element name="Kontakt" type="TOsoba" minOccurs="0" />
    </xs:sequence>
    <xs:attribute name="Nazev" type="xs:string" use="required"/>
    <xs:attribute name="Stat" type="xs:string" use="required"/>
    <xs:attribute name="ICO" type="xs:string"/>
    <xs:attribute name="DIC" type="xs:string"/>
    <xs:attribute name="PSC" type="xs:string"/>
    <xs:attribute name="Telefon" type="xs:string"/>
    <xs:attribute name="Email" type="TEmail"/>
  </xs:complexType>
  <xs:complexType name="TRozpocet">
    <xs:complexContent>
      <xs:extension base="TIdentita">
        <xs:sequence>
          <xs:element name="Objednatel" type="TOrganizace" minOccurs="0"/>
          <xs:element name="Dodavatel" type="TOrganizace" minOccurs="0"/>
          <xs:element name="Objekt" type="TObjekt" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="Polozka" type="TPolozkaRozpoctu" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TUzelRozpoctu">
    <xs:complexContent>
      <xs:extension base="TIdentita">
        <xs:sequence>
          <xs:element name="Zatrideni" type="TTridaRef" minOccurs="0" maxOccurs="unbounded">
            <xs:keyref name="NodeClassKeyRef" refer="ClassKey">
              <xs:selector xpath="."/>
              <xs:field xpath="@Ref"/>
            </xs:keyref>
          </xs:element>
          <xs:element name="Polozka" type="TPolozkaRozpoctu" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="VlastniTyp" use="optional"></xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
    <!--Vyhláška, kořen hierarchie -->
  <xs:complexType name="TObjekt">
    <xs:complexContent>
      <xs:extension base="TUzelRozpoctu">
        <xs:sequence>
          <xs:element name="Objekt" type="TObjekt" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="UzelRozpoctu" type="TUzelRozpoctu" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <!-- TODO: ClassRef? -->
        <!-- Povinne z vyhlasky -->
        <xs:attribute name="KSO" type="xs:string"></xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TPolozkaRozpoctu">
    <xs:complexContent>
      <xs:extension base="TIdentita">
        <xs:sequence>
          <xs:element name="Zatrideni" type="TTridaRef" minOccurs="0" maxOccurs="unbounded">
            <xs:keyref name="ClassKeyRef" refer="ClassKey">
              <xs:selector xpath="."/>
              <xs:field xpath="@Ref"/>
            </xs:keyref>
          </xs:element>
          <xs:element name="CenovaSoustava" type="TCenovaSoustavaRef" minOccurs="0" maxOccurs="1">
            <xs:keyref name="CSRef" refer="CostingSourceKey">
              <xs:selector xpath="."/>
              <xs:field xpath="@Ref"/>
            </xs:keyref>
          </xs:element>
          <xs:element name="Mnozstvi" type="TMnozstvi" minOccurs="1" maxOccurs="1">
            <xs:keyref name="UnitRef" refer="UnitKey">
              <xs:selector xpath="." />
              <xs:field xpath="@MernaJednotka"/>
            </xs:keyref>
          </xs:element>
          <xs:element name="SouvisejiciEntita" type="TExterniEntita" minOccurs="0" maxOccurs="unbounded"></xs:element>
          <xs:element name="KomentarZadavatele" type="xs:string" minOccurs="0"></xs:element>
          <xs:element name="KomentarUchazece" type="xs:string" minOccurs="0"></xs:element>
        </xs:sequence>
        <xs:attribute name="Kod" type="xs:string" use="required"></xs:attribute>
        <xs:attribute name="PoradoveCislo" type="xs:positiveInteger" use="required"></xs:attribute>
        
        <!-- TODO: Potvrdit HSV, PSV, M, ...? -->
        <xs:attribute name="Typ" type="TTypPolozky" use="required"></xs:attribute>
        <xs:attribute name="JednotkovaHmotnost" type="xs:double" use="optional"></xs:attribute>
        <xs:attribute name="JednotkovaDemontazniHmotnost" type="xs:double" use="optional"></xs:attribute>
        <!--TODO: Odkazem na enumeraci v definicích?-->
        <xs:attribute name="SazbaDPH" type="xs:decimal" use="optional"></xs:attribute>
        <xs:attribute name="PrenesenaDPH" type="xs:boolean" use="optional"></xs:attribute>
        <xs:attribute name="PouzeMaterial" type="xs:boolean" use="optional"></xs:attribute>
        
        <xs:attribute name="Varianta" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation xml:lang="cs">
              Odlišení variace od cenové soustavy (především OTSKP)
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        
        <xs:attribute name="Zpusobilost" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation xml:lang="cs">
              Způsobilost pro dotace. Tento údaj slouží především pro vyhodnocení oceněného rozpočtu.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>

        <!--TODO: Změnit na element s definicí toho, kdo jednotkovou cenu dodal, jako samostatna datova vrstva/identita-->
        <xs:attribute name="JednotkovaCena" type="TCena" use="required"></xs:attribute>
        
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TMnozstvi">
    <xs:sequence>
      <xs:element name="Vymera" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="cs">
            Lidsky čitelný vzorec, který vedl k výslednému množství. Není ambicí této verze ORF definovat neutrální formát vzorce.
            Pokud vzorec obsahuje konstanty (figury), měly by být buď substituovány, nebo uvedeny na začátku výpočtového vzorce.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="SkupinaMereni" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="cs">
          Odkaz na metodiku měření
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute type="THodnotaMnozstvi" name="Hodnota" use="required"></xs:attribute>
    <xs:attribute name="MernaJednotka" type="xs:IDREF" use="required"></xs:attribute>
  </xs:complexType>

  <xs:element name="ORF" type="TORF">
    <xs:annotation>
      <xs:documentation>

      </xs:documentation>
    </xs:annotation>
    <xs:key name="ClassKey">
      <xs:selector xpath=".//Trida"/>
      <xs:field xpath="@Id"/>
    </xs:key>
    <xs:key name="CostingSourceKey">
      <xs:selector xpath="Definice/CenovaSoustava"/>
      <xs:field xpath="@Id"/>
    </xs:key>
    <xs:key name="UnitKey">
      <xs:selector xpath="Definice/Unit"/>
      <xs:field xpath="@Id"/>
    </xs:key>
  </xs:element>
</xs:schema>
