  <xs:simpleType name="TNazevJednotkySI">
    <xs:annotation>
      <xs:documentation xml:lang="cs">Názvy jednotek podle ISO 80000-1:2009</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="metre">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: length
            Symbol: m
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="kilogram">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: mass
            Symbol: kg
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="second">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: time
            Symbol: s
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="ampere">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: electric current
            Symbol: A
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="kelvin">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: thermodynamic temperature
            Symbol: K
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="mole">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: amount of substance
            Symbol: mol
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="candela">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: luminous intensity
            Symbol: cd
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="radian">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: plane angle
            Symbol: rad
            Expression: rad = m/m = 1
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="steradian">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: solid angle
            Symbol: sr
            Expression: sr = m2/m2 = 1
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="hertz">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: frequency
            Symbol: Hz
            Expression: Hz = s−1
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="newton">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: force
            Symbol: N
            Expression: N = kg · m/s2
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="pascal">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: pressure
            Symbol: stress
            Expression: Pa	Pa = N/m2
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="joule">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: energy
            Symbol: J
            Expression: J = N · m
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="watt">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: power
            Symbol: W
            Expression: W = J/s
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="coulomb">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: electric charge
            Symbol: C
            Expression: C = A · s
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="volt">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: electric potential difference
            Symbol: V
            Expression: V = W/A
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="farad">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: capacitance
            Symbol: F
            Expression: F = C/V
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="ohm">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: electric resistance
            Symbol: Ω
            Expression: Ω = V/A
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="siemens">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: electric conductance
            Symbol: S
            Expression: S = Ω−1
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="weber">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: magnetic flux
            Symbol: Wb
            Expression: Wb = V · s
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="tesla">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: magnetic flux density
            Symbol: T
            Expression: T = Wb/m2
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="henry">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: inductance
            Symbol: H
            Expression: H = Wb/A
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="degree_Celsius">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: Celsius temperature
            Symbol: ºC
            Expression: ºC = K
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="lumen">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: luminous flux
            Symbol: lm
            Expression: lm = cd · sr
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="lux">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: illuminance
            Symbol: lx
            Expression: lx = lm/m2
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="becquerel">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: activity (of a radionuclide)
            Symbol: Bq
            Expression: Bq = s−1
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="gray">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: absorbed dose
            Symbol: Gy
            Expression: Gy = J/kg
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="sievert">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: dose equivalent
            Symbol: Sv
            Expression: Sv = J/kg
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

      <xs:enumeration value="katal">
        <xs:annotation>
          <xs:documentation xml:lang="en">
            Quantity: catalytic activity
            Symbol: kat
            Expression: kat = mol/s
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>

    </xs:restriction>
  </xs:simpleType>
