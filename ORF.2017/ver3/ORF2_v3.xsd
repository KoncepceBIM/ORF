			<xs:element name="vol_udaje" type="cTyp_VolitelneUdaje" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="cTyp_Osoba">
		<xs:annotation>
			<xs:documentation>Typ elementu OSOBA (kontaktní osoba firmy)</xs:documentation>
