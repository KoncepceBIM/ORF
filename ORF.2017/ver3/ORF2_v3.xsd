			<xs:element name="polozka_ciselniku" type="cTyp_PolozkaCiselniku"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="cTyp_VolitelnyUdaj">
		<xs:annotation>
			<xs:documentation>Typ elementu VOLITELNÝ ÚDAJ - hodnota</xs:documentation>
		</xs:annotation>
